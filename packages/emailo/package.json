    "@tiptap/core": "^2.7.1",
    "@tiptap/extension-color": "^2.7.1",
    "@tiptap/extension-list-item": "^2.7.1",
    "@tiptap/extension-text-style": "^2.7.1",
    "@tiptap/pm": "^2.7.1",
    "@tiptap/react": "^2.7.1",
    "@tiptap/starter-kit": "^2.7.1",
    "@tiptap/suggestion": "^2.7.1",
    "clsx": "^2.1.1",
